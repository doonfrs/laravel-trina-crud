{
    "name": "trinavo/laravel-trina-crud",
    "description": "Laravel package for rapid CRUD API generation with built-in authorization and validation.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Feras Abdalrahman",
            "email": "doon@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^12.0",
        "livewire/livewire": "^3.6",
        "symfony/yaml": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Trinavo\\TrinaCrud\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Trinavo\\TrinaCrud\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Trinavo\\TrinaCrud\\Providers\\TrinaCrudServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "laravel/framework": "^12.0",
        "phpunit/phpunit": "^11.0",
        "mockery/mockery": "^1.5",
        "orchestra/testbench": "^10.0",
        "spatie/laravel-permission": "^6.16",
        "trinavo/laravel-ownable": "dev-main"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}